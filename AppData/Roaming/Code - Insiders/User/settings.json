{
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[powershell]": {
        "editor.wordSeparators": "`~!@#%^&*()-=+[{]}\\|;:'\",.<>/?"
    },
    "accessibility.hideAccessibleView": true,
    "accessibility.verbosity.terminal": false,
    "debug.console.fontFamily": "Delugia",
    "editor.cursorBlinking": "phase",
    "editor.defaultFoldingRangeProvider": "ms-vscode.powershell",
    "editor.defaultFormatter": "ms-vscode.powershell",
    "editor.fontFamily": "Delugia",
    "editor.fontLigatures": true,
    "editor.fontSize": 16,
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.minimap.enabled": false,
    "editor.snippetSuggestions": "bottom",
    "editor.suggest.snippetsPreventQuickSuggestions": true,
    "editor.tabCompletion": "on",
    "errorLens.delay": 10000,
    "files.associations": {
        ".chezmoi*.yaml": "helm"
    },
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 10000,
    "files.trimTrailingWhitespace": true,
    "git.autofetch": true,
    "git.enableSmartCommit": true,
    "gitlens.views.compare.location": "scm",
    "gitlens.views.fileHistory.location": "scm",
    "gitlens.views.lineHistory.location": "scm",
    "gitlens.views.repositories.location": "scm",
    "gitlens.views.search.location": "scm",
    "indentRainbow.indicatorStyle": "light",
    "launch": {
        "compounds": [],
        "configurations": [
            {
                "name": "PS: Interactive",
                "presentation": {
                    "group": "Powershell"
                },
                "request": "launch",
                "type": "PowerShell"
            },
            {
                "args": [
                    "${command:SpecifyScriptArgs}"
                ],
                "name": "PS: Pester",
                "presentation": {
                    "group": "Powershell"
                },
                "request": "launch",
                "script": "Invoke-Pester",
                "type": "PowerShell"
            }
        ]
    },
    "pester.suppressCodeLensNotice": true,
    "powershell.analyzeOpenDocumentsOnly": true,
    "powershell.codeFormatting.autoCorrectAliases": true,
    "powershell.codeFormatting.avoidSemicolonsAsLineTerminators": true,
    "powershell.codeFormatting.newLineAfterCloseBrace": false,
    "powershell.codeFormatting.newLineAfterOpenBrace": false,
    "powershell.codeFormatting.openBraceOnSameLine": true,
    "powershell.codeFormatting.pipelineIndentationStyle": "IncreaseIndentationForFirstPipeline",
    "powershell.codeFormatting.trimWhitespaceAroundPipe": true,
    "powershell.codeFormatting.useConstantStrings": true,
    "powershell.codeFormatting.whitespaceBetweenParameters": true,
    "powershell.helpCompletion": "LineComment",
    "powershell.integratedConsole.showOnStartup": false,
    "powershell.integratedConsole.suppressStartupBanner": true,
    "powershell.pester.codeLens": false,
    "settingsSync.ignoredSettings": [
        "-git.defaultCloneDirectory",
        "powershell.powerShellDefaultVersion"
    ],
    "terminal.external.windowsExec": "wt",
    "terminal.integrated.accessibleViewFocusOnCommandExecution": false,
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cursorStyle": "underline",
    "terminal.integrated.cursorStyleInactive": "underline",
    "terminal.integrated.defaultProfile.windows": "PowerShell",
    "terminal.integrated.enablePersistentSessions": false,
    "terminal.integrated.fontFamily": "Delugia",
    "terminal.integrated.fontLigatures.enabled": true,
    "terminal.integrated.fontSize": 16,
    "terminal.integrated.hideOnStartup": "whenEmpty",
    "terminal.integrated.persistentSessionReviveProcess": "never",
    "terminal.integrated.scrollback": 9999,
    "todo-tree.general.tags": [
        "BUG",
        "HACK",
        "FIXME",
        "TODO",
        "DEBUG"
    ],
    "update.enableWindowsBackgroundUpdates": false,
    "vsicons.associations.files": [
        {
            "extensions": [
                "parameters.json"
            ],
            "format": "svg",
            "icon": "config"
        },
        {
            "extensions": [
                "tests.ps1"
            ],
            "format": "svg",
            "icon": "test"
        },
        {
            "extensions": [
                "adml",
                "admx",
                "clixml"
            ],
            "format": "svg",
            "icon": "xml"
        }
    ],
    "vsicons.associations.folders": [
        {
            "extends": "dist",
            "extensions": [
                "BuildOutput",
                "Output"
            ],
            "format": "svg",
            "icon": "dist"
        },
        {
            "extends": "helper",
            "extensions": [
                "BuildHelpers"
            ],
            "format": "svg",
            "icon": "helper"
        }
    ],
    "vsicons.dontShowNewVersionMessage": true,
    "window.newWindowDimensions": "maximized",
    "window.newWindowProfile": "Default",
    "window.titleSeparator": " | ",
    "workbench.colorCustomizations": {
        "errorLens.annotationPrefix": [
            "üí• ",
            "‚ö†Ô∏è ",
            "üîπ ",
            "üí° "
        ],
        "errorLens.errorBackground": "#e4545410",
        "errorLens.errorForeground": "#ff646440",
        "errorLens.errorMessageBackground": "#e4545405",
        "errorLens.warningBackground": "#ff942f10",
        "errorLens.warningForeground": "#fa973a70",
        "errorLens.warningMessageBackground": "#fff00005"
    },
    "workbench.colorTheme": "Mintchoc (rainglow)",
    "workbench.editor.empty.hint": "hidden",
    "workbench.iconTheme": "vscode-icons",
    "workbench.preferredDarkColorTheme": "Heroku (rainglow)",
    "workbench.sideBar.location": "right",
    "workbench.startupEditor": "none",
    "powershell.debugging.createTemporaryIntegratedConsole": true,
    "chat.commandCenter.enabled": false,
    "editor.suggest.showDeprecated": false,
    "powershell.trace.server": "verbose",
    "powershell.developer.traceDap": true,
    "terminal.integrated.suggest.enabled": false,
    "terminal.integrated.stickyScroll.enabled": false,
    "scm.diffDecorationsGutterPattern": {
        "modified": false
    },
    "scm.diffDecorationsGutterAction": "none",
    "powershell.debugging.executeMode": "Call",
    "files.dialog.defaultPath": "C:\\Repositories",
    "editor.minimap.autohide": "scroll",
    "security.workspace.trust.untrustedFiles": "open",
    "[xml]": {
        "editor.defaultFormatter": "DotJoshJohnson.xml"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "workbench.secondarySideBar.defaultVisibility": "hidden",
    "github.copilot.nextEditSuggestions.enabled": true,
    "chat.checkpoints.showFileChanges": true,
    "powershell.promptToUpdatePowerShell": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "chat.tools.urls.autoApprove": {
        "https://techcommunity.microsoft.com": {
            "approveRequest": false,
            "approveResponse": true
        }
    },
    "chat.tools.terminal.outputLocation": "chat",
    "editor.pasteAs.preferences": [
        "text.plain"
    ]
}